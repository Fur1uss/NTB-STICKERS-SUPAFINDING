{
  "name": "ntb-sticker-supafinding",
  "private": true,
  "version": "1.0.0",
  "description": "NTB Sticker Supafinding - Full Stack Application",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:vercel": "vercel dev",
    "dev:full": "concurrently --names \"VERCEL,FRONTEND\" --prefix-colors \"blue,green\" \"npm run dev:vercel\" \"npm run dev:frontend\"",
    "vercel:login": "vercel login",
    "start": "npm run dev",
    "build": "cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:frontend"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ntb-sticker-supafinding.git"
  },
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.52.1",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
